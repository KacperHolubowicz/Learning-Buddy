{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseType": "Testing",
  "ConnectionStrings": {
    "Testing": "{TESTING}",
    "Docker": "{DOCKER}"
  },
  "Security": {
    "Encryption": {
      "Iterations": 4000,
      "KeyBytes": 32
    },
    "JwtKey": "{KEY}",
    "Issuer": "https://localhost:7276",
    "Audience": "https://localhost:7276",
    "AccessTokenMinutes": 5,
    "RefreshTokenDays": 3
  }
}
